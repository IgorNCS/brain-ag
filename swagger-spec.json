  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "LoginDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "example": "Fulano"
          },
          "password": {
            "type": "string",
            "example": "senhasecreta123"
          }
        },
        "required": [
          "username",
          "password"
        ]
      },
      "LoginResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "example": true
          },
          "access_token": {
            "type": "string",
            "example": "eyJhbGciOiJIUzUxMiIsInR5cCIgOiAiSldUIiwia2lkIjoiMjA5ZjhlNmMtZTVhNC00ZmFmLWE5ODItODkyMmRiNjVhN2Y3In0"
          },
          "refresh_token": {
            "type": "string",
            "example": "eyJhbGciOiJIUzUxMiIsInR5cCIgOiAiSldUIiwia2lkIjoiMjA5ZjhlNmMtZTVhNC00ZmFmLWE5ODItODkyMmRiNjVhN2Y3In0"
          },
          "expires_in": {
            "type": "number",
            "example": 1800
          }
        },
        "required": [
          "success",
          "access_token",
          "refresh_token",
          "expires_in"
        ]
      },
      "RefreshTokenDto": {
        "type": "object",
        "properties": {
          "refresh_token": {
            "type": "string",
            "example": "eyJhbGciOiJIUzUxMiIsInR5cCIgOiAiSldUIiwia2lkIiA6ICI2MjYxYjBmYS04ZjQ4LTQ0NzUtODhiNi0xMjIwNjg1YjZiZjUifQ..U7MmoO0wapldVzTJquMajLSNWZwlKGdxAgc05-V3bDOvVgosCesGrCcanmZRUBCc_IT7JNBb7nStVWR3L6P3Hg"
          }
        },
        "required": [
          "refresh_token"
        ]
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Fulano de tal"
          },
          "email": {
            "type": "string",
            "example": "fulano@gmail.com"
          },
          "password": {
            "type": "string",
            "example": "senhasecreta123"
          },
          "role": {
            "type": "string",
            "example": "costumer",
            "description": "Apenas ADMIN pode enviar"
          },
          "cpfCnpj": {
            "type": "number",
            "example": "12345678900"
          }
        },
        "required": [
          "name",
          "email",
          "password",
          "cpfCnpj"
        ]
      },
      "UpdateUserDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Fulano de tal"
          },
          "email": {
            "type": "string",
            "example": "fulano@gmail.com"
          },
          "password": {
            "type": "string",
            "example": "senhasecreta123"
          },
          "role": {
            "type": "string",
            "example": "costumer",
            "description": "Apenas ADMIN pode enviar"
          },
          "cpfCnpj": {
            "type": "number",
            "example": "12345678900"
          }
        }
      },
      "CreateFarmDTO": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Colheita feliz"
          },
          "city": {
            "type": "string",
            "example": "New York"
          },
          "state": {
            "type": "string",
            "example": "NY"
          },
          "totalArea": {
            "type": "number",
            "example": 100.5
          },
          "arableArea": {
            "type": "number",
            "example": 80
          },
          "vegetationArea": {
            "type": "number",
            "example": 20.5
          },
          "userId": {
            "type": "string",
            "example": "e7d3c6c4-3b6d-4e7a-9eae-64c93f9f7f4f"
          }
        },
        "required": [
          "name",
          "city",
          "state",
          "totalArea",
          "arableArea",
          "vegetationArea"
        ]
      },
      "PaginationBodyDTO": {
        "type": "object",
        "properties": {
          "userId": {
            "example": [
              "b7f0c5a0-0a1e-11ec-9621-0242ac130002"
            ],
            "description": "Client Id.",
            "type": "array",
            "items": {
              "type": "array"
            }
          },
          "startDate": {
            "type": "string",
            "example": "2022-01-01",
            "pattern": "/^d{4}-d{2}-d{2}$/",
            "description": "Restricts the query to authorizations with Start Date greater than or equal to the informed. Format: yyyy-mm-dd"
          },
          "endDate": {
            "type": "string",
            "example": "2022-01-01",
            "pattern": "/^d{4}-d{2}-d{2}$/",
            "description": "Restricts the query to authorizations with End Date less than or equal to the informed. Format: yyyy-mm-dd"
          },
          "relations": {
            "type": "array",
            "example": [
              "user"
            ],
            "description": "Relations that will be returned.",
            "items": {
              "type": "string",
              "enum": [
                "user",
                "farm"
              ]
            }
          }
        }
      },
      "UpdateFarmDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Colheita feliz"
          },
          "city": {
            "type": "string",
            "example": "New York"
          },
          "state": {
            "type": "string",
            "example": "NY"
          },
          "totalArea": {
            "type": "number",
            "example": 100.5
          },
          "arableArea": {
            "type": "number",
            "example": 80
          },
          "vegetationArea": {
            "type": "number",
            "example": 20.5
          }
        }
      }
    }
  }
}